{
  "cli": {
    "version": ">= 12.6.2",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_URL": "http://dapp.bableto.site/api"
      }
    }
  },
  "test": {
    "env": {
      "EXPO_PUBLIC_URL": "http://dapp.bableto.site/api"
    }
  },
  "submit": {
    "production": {}
  }
}
